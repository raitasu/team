{
  "private": true,
  "name": "team-front",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/cybergizer-hq/team-front.git"
  },
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "eject": "react-scripts eject",
    "format": "eslint src --fix && npm run format:package-json",
    "format:package-json": "node scripts/validatePackageJson.mjs --fix",
    "gen:theme-typings": "chakra-cli tokens /src/shared/ui/theme/theme.ts",
    "postinstall": "husky install && npm run gen:theme-typings",
    "lint": "eslint src && npm run lint:package-json",
    "lint:package-json": "node scripts/validatePackageJson.mjs",
    "semantic-release": "semantic-release",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@chakra-ui/react": "2.3.4",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@fontsource/roboto": "4.5.8",
    "framer-motion": "7.3.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "react-router-dom": "6.4.1",
    "react-scripts": "5.0.1",
    "typescript": "4.8.3"
  },
  "devDependencies": {
    "@chakra-ui/cli": "2.1.8",
    "@chakra-ui/styled-system": "2.3.3",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@emotion/eslint-plugin": "11.10.0",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addon-storyshots": "6.5.12",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@storybook/node-logger": "6.5.12",
    "@storybook/preset-create-react-app": "4.1.2",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.43",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "babel-plugin-named-exports-order": "0.0.2",
    "chalk": "5.0.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-prettier": "4.2.1",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-package-json": "2.6.4",
    "prop-types": "15.8.1",
    "semantic-release": "19.0.5",
    "webpack": "5.74.0"
  },
  "engines": {
    "node": "16.17.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
