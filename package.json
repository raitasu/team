    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.9.1",
    "execa": "6.1.0",
    "fs-extra": "11.1.0",
    "husky": "8.0.3",
