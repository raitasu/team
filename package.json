{
  "private": true,
  "name": "team-front",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cybergizer-hq/team-front.git"
  },
  "bugs": {
    "url": "https://github.com/cybergizer-hq/team-front/issues"
  },
  "main": "src/main.tsx",
  "scripts": {
    "audit:deadcode": "npx ts-prune >deadcode.txt",
    "audit:deadcode:ci": "./tools/scripts/ci/checkDeadCode.sh",
    "build": "vite build",
    "build-storybook": "build-storybook",
    "build:preview": "vite preview",
    "build:staging": "cross-env NODE_ENV=production npm run build -- --mode staging",
    "build:staging:with-mock-server": "cross-env VITE_USE_MOCK_SERVER=true npm run build:staging",
    "chromatic": "npx chromatic",
    "format": "node ./tools/scripts/qa/lint.mjs --fix",
    "gen:theme-typings": "chakra-cli tokens /src/shared/ui/theme/theme.ts",
    "postinstall": "husky install && npm run gen:theme-typings && npm run msw:init",
    "lint": "node ./tools/scripts/qa/lint.mjs",
    "msw:init": "msw init ./public",
    "semantic-release": "semantic-release",
    "start": "vite",
    "start:with-mock-server": "cross-env VITE_USE_MOCK_SERVER=true vite",
    "storybook": "start-storybook -p 6006",
    "test": "vitest",
    "test:ci": "vitest run --coverage --outputFile.junit=./junit/vitest.xml"
  },
  "dependencies": {
    "@chakra-ui/react": "2.3.6",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/roboto": "4.5.8",
    "@reduxjs/toolkit": "1.8.6",
    "@tanstack/react-table": "8.5.22",
    "async-mutex": "0.4.0",
    "framer-motion": "7.6.4",
    "i18next": "22.0.4",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.0.0",
    "react-icons": "4.6.0",
    "react-redux": "8.0.4",
    "react-router-dom": "6.4.3",
    "react-use": "17.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/eslint-parser": "7.19.1",
    "@chakra-ui/cli": "2.1.8",
    "@chakra-ui/styled-system": "2.3.4",
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@emotion/eslint-plugin": "11.10.0",
    "@faker-js/faker": "7.6.0",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-interactions": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-vite": "0.2.5",
    "@storybook/node-logger": "6.5.13",
    "@storybook/react": "6.5.13",
    "@swc/core": "1.3.11",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.187",
    "@types/node": "16.11.43",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/coverage-c8": "0.24.5",
    "chakra-react-select": "4.4.0",
    "chalk": "5.1.2",
    "chromatic": "6.11.4",
    "confusing-browser-globals": "1.0.11",
    "cross-env": "7.0.3",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.9.1",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "jsdom": "20.0.2",
    "lint-staged": "13.0.3",
    "msw": "0.47.4",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prettier-package-json": "2.7.0",
    "prop-types": "15.8.1",
    "semantic-release": "19.0.5",
    "ts-prune": "0.10.3",
    "typescript": "4.8.4",
    "vite": "3.2.2",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "0.24.5",
    "vitest-github-actions-reporter": "0.9.0"
  },
  "engines": {
    "node": "16.18.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
